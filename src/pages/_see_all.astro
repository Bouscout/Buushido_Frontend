---
import MainLayout from "../layouts/main_layout.astro";
import NavigationBar from "../layouts/navigation_bar.astro";
import Footer from "../layouts/footer.astro";
import Voir_tout from "../components/voir_tout/voir_tout";

const series_data = await fetch("https://buushido.ml/api/all/")
const series = await series_data.json()

---
<link rel="preload stylesheet" href="/styles/voir_tout/main.css" as="style" type="text/css">
<link rel="preload stylesheet" href="/styles/voir_tout/small.css" as="style" type="text/css" media="screen and (max-width: 800px)">
<link rel="preload stylesheet" href="/styles/voir_tout/load.css" as="style"  type="text/css">


<MainLayout>
    <NavigationBar/>
    <Voir_tout data={series} client:visible />
    <Footer />
</MainLayout>